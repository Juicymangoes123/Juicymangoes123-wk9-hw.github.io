{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 450,
    "title": "Patterns of migration into Australia",
    "projection": {"type": "equirectangular"},
    "layer":[
        {
            "data": {
                "url": "au-states.topojson",
                "format": {"type": "topojson", "feature": "ne_10m_admin_1_states_provinces"}
            },
            "transform": [
                {
                    "lookup": "properties.name",
                    "from": {
                        "data": {
                            "url": "overseas-migration.csv"
                        },
                        "key": "State",
                        "fields": ["Y2022_23", "Y2022_23_total"]
                    }
                },
                {"calculate": "datum.Y2022_23/datum.Y2022_23_total", "as": "% of annual national migration total"}
            ],
            "mark": {"type": "geoshape"},
            "encoding": {
                "color": {
                    "field": "% of annual national migration total",
                    "type": "quantitative",
                    "scale": {
                        "type": "threshold",
                        "domain": [0.05, 0.10, 0.20, 0.30],
                        "range": ["#edf8fb","#b2e2e2","#66c2a4","#2ca25f","#006d2c"]
                    }
                }
            }
        },
        {
            "data": {
                "url": "au-states.topojson",
                "format": {"type": "topojson", "feature": "ne_110m_graticules_30"}
            },
            "mark": {"type": "geoshape", "stroke": "red"}
        }
    ]
}